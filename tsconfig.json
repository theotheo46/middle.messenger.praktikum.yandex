{
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "outDir": "dist",
        "target": "es2016",
        "lib" : [
            "ESNext",
            "DOM"
        ],
        "declaration": false,
        "module": "commonjs",
        "strictNullChecks": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,  
        "noUnusedParameters": true,
        "esModuleInterop": true,
        "types": [ "node" ]
   }
} 

